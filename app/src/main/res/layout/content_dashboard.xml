<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_margin="5dp"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/app_bar_dashboard"
    tools:context=".Desgin.DashboardActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:elevation="3dp"
        android:orientation="horizontal">
        <android.support.v7.widget.CardView
            style="@style/Cardview"
            android:layout_width="0dp"
            android:layout_height="130dp">

        <Button
            android:id="@+id/dashbpard_btn_dimond"
            android:layout_width="match_parent"
            android:text="Dimond"
            android:layout_height="130dp"
            android:paddingTop="10dp"
            android:drawableTop="@mipmap/ic_diamond"
            android:background="@android:color/transparent"
            android:layout_weight="1"/>


        </android.support.v7.widget.CardView>
        <View
            android:layout_width="0.2dp"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="1dp"
            android:layout_height="match_parent"
            android:background="@color/light_blue_300"/>

        <android.support.v7.widget.CardView
            style="@style/Cardview"
            android:layout_width="0dp"
            android:layout_height="130dp">

        <Button
            android:id="@+id/dashbpard_btn_god"
            android:layout_width="match_parent"
            android:text="GOD"
            android:layout_height="130dp"
            android:paddingTop="10dp"
            android:drawableTop="@mipmap/ic_god"
            android:background="@android:color/transparent"
            android:layout_weight="1"/>
        </android.support.v7.widget.CardView>
        <View
            android:layout_width="0.2dp"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="1dp"
            android:layout_height="match_parent"
            android:background="@color/light_blue_300">

        </View>
        <android.support.v7.widget.CardView
            style="@style/Cardview"
            android:layout_width="0dp"
            android:layout_height="130dp">

        <Button
            android:id="@+id/dashbpard_btn_heart"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:text="Heart Shape"
            android:paddingTop="10dp"
            android:drawableTop="@mipmap/ic_heartshape"
            android:background="@android:color/transparent"
            android:layout_weight="1"/>
        </android.support.v7.widget.CardView>
    </LinearLayout>

<View
    android:layout_width="match_parent"
    android:layout_height="0.2dp"
    android:background="@color/light_blue_300">

</View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="3dp"
        android:orientation="horizontal">
        <android.support.v7.widget.CardView
            style="@style/Cardview"
            android:layout_width="0dp"
            android:layout_height="130dp">

        <Button
            android:id="@+id/dashbpard_btn_picture"
            android:layout_width="match_parent"
            android:text="Picture"
            android:layout_height="130dp"
            android:paddingTop="15dp"
            android:drawableTop="@mipmap/ic_picture"
            android:background="@android:color/transparent"
            android:layout_weight="1"/>
        </android.support.v7.widget.CardView>
        <View
            android:layout_width="0.2dp"
            android:layout_height="match_parent"
            android:layout_marginTop="2dp"
            android:background="@color/light_blue_300"/>

        <android.support.v7.widget.CardView
            style="@style/Cardview"
            android:layout_width="0dp"
            android:layout_height="130dp">

        <Button
            android:id="@+id/dashbpard_btn_premium"
            android:layout_width="match_parent"
            android:text="Premium"
            android:layout_height="130dp"
            android:paddingTop="14dp"
            android:drawableTop="@mipmap/ic_premium"
            android:background="@android:color/transparent"
            android:layout_weight="1"/>
        </android.support.v7.widget.CardView>
        <View
            android:layout_width="0.2dp"
            android:layout_height="120dp"
            android:layout_marginTop="1dp"
            android:background="@color/light_blue_300"/>

        <android.support.v7.widget.CardView
            style="@style/Cardview"
            android:layout_width="0dp"
            android:layout_height="130dp">

        <Button
            android:id="@+id/dashbpard_btn_simple"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:text="Simple"
            android:paddingTop="15dp"
            android:drawableTop="@mipmap/ic_simple"
            android:background="@android:color/transparent"
            android:layout_weight="1"/>
        </android.support.v7.widget.CardView>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.2dp"
        android:background="@color/light_blue_300"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal">
        <android.support.v7.widget.CardView
            style="@style/Cardview"
            android:layout_width="0dp"
            android:elevation="3dp"
            android:layout_height="130dp">

        <Button
            android:id="@+id/dashbpard_btn_small"
            android:layout_width="match_parent"
            android:text="Small"
            android:layout_height="130dp"
            android:paddingTop="15dp"
            android:drawableTop="@mipmap/ic_small"
            android:background="@android:color/transparent"
            android:layout_weight="1"/>
        </android.support.v7.widget.CardView>
        <View
            android:layout_width="0.2dp"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="1dp"
            android:layout_height="match_parent"
            android:background="@color/light_blue_300"/>

        <Button
            android:id="@+id/dashbpard_btn_contact"
            android:layout_width="0dp"
            android:text="Doctor login"
            android:layout_height="110dp"
            android:paddingTop="10dp"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_weight="1"/>

        <Button
            android:id="@+id/admin_btn_login"
            android:layout_width="0dp"
            android:layout_height="110dp"
            android:visibility="invisible"
            android:text="Admin Login"
            android:paddingTop="10dp"
            android:background="@android:color/transparent"
            android:layout_weight="1"/>

    </LinearLayout>



</LinearLayout>